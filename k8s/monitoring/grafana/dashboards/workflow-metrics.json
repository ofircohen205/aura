{
  "dashboard": {
    "title": "LangGraph Workflow Metrics",
    "tags": ["workflow", "langgraph", "metrics", "struggle", "audit"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Workflow Executions Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(rate(workflow_executions_total[5m])) by (workflow_type, status)",
            "legendFormat": "{{workflow_type}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Executions/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "title": "Workflow Duration",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(workflow_duration_seconds_bucket[5m])) by (le, workflow_type))",
            "legendFormat": "p50 - {{workflow_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(workflow_duration_seconds_bucket[5m])) by (le, workflow_type))",
            "legendFormat": "p95 - {{workflow_type}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(workflow_duration_seconds_bucket[5m])) by (le, workflow_type))",
            "legendFormat": "p99 - {{workflow_type}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration (seconds)"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "title": "Struggle Detections",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(increase(struggle_detections_total{result=\"struggling\"}[1h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "yellow"
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Lesson Recommendations Generated",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(increase(lesson_recommendations_generated_total[1h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Struggle Detection Results",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(rate(struggle_detections_total[5m])) by (result)",
            "legendFormat": "{{result}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Audit Execution Status",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "targets": [
          {
            "expr": "sum(rate(audit_executions_total[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Executions/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 7,
        "title": "Audit Violations Detected",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(audit_violations_detected_bucket[5m])) by (le))",
            "legendFormat": "p50 violations",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(audit_violations_detected_bucket[5m])) by (le))",
            "legendFormat": "p95 violations",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(audit_violations_detected_bucket[5m])) by (le))",
            "legendFormat": "p99 violations",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Violations"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 8,
        "title": "Violations by Type",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "sum(rate(audit_violations_by_type_total[5m])) by (violation_type)",
            "legendFormat": "{{violation_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Violations/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 9,
        "title": "Files Processed per Audit",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(audit_files_processed_bucket[5m])) by (le))",
            "legendFormat": "p50 files",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(audit_files_processed_bucket[5m])) by (le))",
            "legendFormat": "p95 files",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(audit_files_processed_bucket[5m])) by (le))",
            "legendFormat": "p99 files",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Files"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 10,
        "title": "Workflow Failures",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 28
        },
        "targets": [
          {
            "expr": "sum(rate(workflow_failures_total[5m])) by (workflow_type, error_type)",
            "legendFormat": "{{workflow_type}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Failures/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 11,
        "title": "Struggle Workflow Input Metrics",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 28
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(struggle_workflow_edit_frequency_bucket[5m])) by (le))",
            "legendFormat": "p50 edit frequency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(struggle_workflow_error_count_bucket[5m])) by (le))",
            "legendFormat": "p50 error count",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Value"
          },
          {
            "format": "short"
          }
        ]
      }
    ]
  }
}
